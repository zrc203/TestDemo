<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 通用的无状态的 -->
<mapper namespace="AreaMapper">
	<insert id="insertArea">
		insert into area values (#{id},#{name},#{parentId})
	</insert>
	<insert id="insertMusic"  parameterType="map">
		INSERT INTO music (id, type, name, pid, pname, url, size) VALUES
		<foreach collection="list" item="item" index="index" separator=",">
		(#{item.id},#{item.type},#{item.name},#{item.pid},#{item.pname},#{item.url},#{item.size})
		</foreach>
		 
	</insert>
	
</mapper>
